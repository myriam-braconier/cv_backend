{
    "version": 2,
    "builds": [
      {
        "src": "app.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/app.js"
      }
    ],
    "outputDirectory": ".",
    "headers": [
      {
        "source": "/api/(.*)",
      }
    ]
  }